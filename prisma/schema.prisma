// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
}

model Session {
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?
}

model InvData {
  variantId       String  @unique
  inventoryId     String  @unique
  title           String
  color           String
  sku             String  @id
  fechaDisponible String?
}

model statusData {
  id              String  @unique
  variantId       String  @unique
  title           String
  color           String
  sku             String  @id
  status          String?
  price           Float?
}
